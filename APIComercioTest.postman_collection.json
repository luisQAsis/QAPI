				"type": "text/javascript",
				"exec": [
					"var today = new Date();",
					"var dd = today.getDate();",
					"var mm = today.getMonth()+1; //January is 0!",
					"var yyyy = today.getFullYear();",
					"var h = today.getHours();",
					"var m = today.getMinutes();",
					"var s =today.getSeconds();",
					"",
					"if(h<10) {",
					"    h = '0'+h",
					"}",
					"",
					"if(m<10) {",
					"    m = '0'+m",
					"} ",
					"",
					"if(s<10) {",
					"    s = '0'+s",
					"} ",
					"",
					"if(dd<10) {",
					"    dd = '0'+dd",
					"} ",
					"",
					"if(mm<10) {",
					"    mm = '0'+mm",
					"} ",
					"",
					"today = yyyy + '-' + mm + '-' + dd + 'T' + h + ':' + m + ':' + s;",
					"",
					"",
					"var hash = CryptoJS.SHA256('1d069cd9ee6c4b414a0cecfe76affc7b' + today + 'a53dd35be3789441fecab2d0b8fa8e98');",
					"",
					"",
					"postman.setEnvironmentVariable('hash256', hash);",
					"postman.setEnvironmentVariable('time', today);"
				]
			}
		},
